<mvc:View controllerName="logaligroup.sapui5.controller.App" 
          xmlns="sap.m" 
          xmlns:mvc="sap.ui.core.mvc"
          xmlns:core="sap.ui.core" 
          displayBlock="true">
    <Shell id="_IDGenShell1">
        <App id="_IDGenApp1">
            <pages>
                <Page id="_IDGenPage1" title="{i18n>appTitle}">
                <headerContent>
                <Button icon="sap-icon://action"  id="helloDialogHeader"  
                                press=".onOpenDialogHeader" 
                                class="sapUiSmallMarginEnd"/>
                                
                    </headerContent>
                    <content>
                        <mvc:XMLView id="_IDGenXMLView1" viewName="logaligroup.sapui5.view.HelloPanel" />
                      <core:Fragment id="_IDGenFragment11" fragmentName="logaligroup.sapui5.view.Form01" type="XML" />  
                    </content>
                </Page>
            </pages>
        </App>
    </Shell>
</mvc:View>
